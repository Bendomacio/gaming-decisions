{
  "crons": [
    {
      "path": "/api/cron/sync-libraries",
      "schedule": "0 6 * * *"
    },
    {
      "path": "/api/cron/discover-games",
      "schedule": "0 12 * * 1"
    },
    {
      "path": "/api/cron/sync-player-counts",
      "schedule": "0 */6 * * *"
    },
    {
      "path": "/api/cron/sync-trending",
      "schedule": "0 3 * * *"
    }
  ],
  "routes": [
    { "handle": "filesystem" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
